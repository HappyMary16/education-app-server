<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet author="mborodin" id="003.10">
        <preConditions onFail="MARK_RAN">
            <not>
                <foreignKeyConstraintExists foreignKeyName="fk_teacher_degree"/>
            </not>
        </preConditions>

        <addForeignKeyConstraint baseTableName="teachers"
                                 baseColumnNames="science_degree_id"
                                 constraintName="fk_teacher_degree"
                                 referencedTableName="science_degrees"
                                 referencedColumnNames="id"/>
    </changeSet>
</databaseChangeLog>