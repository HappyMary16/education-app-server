<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet author="mborodin" id="004.08">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="study_group_file"/>
            </not>
        </preConditions>
        <createTable tableName="study_group_file">
            <column name="id" type="INTEGER">
                <constraints nullable="false"/>
            </column>

            <column name="study_group_id" type="INTEGER">
                <constraints nullable="false"/>
            </column>
            <column name="file_id" type="INTEGER">
                <constraints nullable="false"/>
            </column>
            <column name="date_add_access" type="TIMESTAMP(6) WITHOUT TIME ZONE">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>